{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "extends": [
    "react-app",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:import/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["warn", { "allow": ["error"] }],
    "import/first": "error"
  }
}
